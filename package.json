{
  "name": "cron-service",
  "version": "0.1.0",
  "license": "MIT",
  "type": "commonjs",
  "author": "Faraz Patankar",
  "main": "dist/index.js",
  "module": "dist/cron-service.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "rm -rf dist/ && tsc",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/cron-service.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cron-service.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/crypto-js": "^4.1.1",
    "@types/cryptojs": "^3.1.30",
    "@types/node-cron": "^3.0.2",
    "dts-cli": "^1.6.0",
    "husky": "^8.0.1",
    "size-limit": "^8.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "base32.js": "^0.1.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "node-cron": "^3.0.2",
    "tsc": "^2.0.4"
  }
}
